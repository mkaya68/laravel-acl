{
    "name": "slynova/laravel-acl",
    "description": "Light-weight role-based permissions for Laravel 5 built in Auth system.",
    "license": "MIT",
    "keywords": ["laravel", "acl", "permissions", "roles", "security"],
    "support": {
        "issues": "https://github.com/slynova-org/laravel-acl/issues",
        "source": "https://github.com/slynova-org/laravel-acl"
    },
    "authors": [
        {
            "name": "Romain Lanz",
            "homepage": "https://github.com/RomainLanz",
            "email": "lanz.romain@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/console": "5.1.*",
        "illuminate/database": "5.1.*",
        "illuminate/support": "5.1.*"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~4.0",
        "squizlabs/php_codesniffer": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Slynova\\Acl\\": "src/Acl"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Slynova\\Acl\\Tests\\": "tests"
        }
    }
}
